<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
   
  <PropertyGroup>
    <DevArea>MetaCore</DevArea>
    <AssemblyName>Meta.Core.Services</AssemblyName>
    <TargetName>$(AssemblyName)</TargetName>
    <DevToolDir>$(DevTools)\</DevToolDir>
    <TopDir>$(DevAreaRoot)\$(DevArea)\</TopDir>
    <TopDevDir>$(TopDir)</TopDevDir>
    <TopConfigDir>$(TopDir).config\</TopConfigDir>
    <AreaLabel>metacore</AreaLabel>
  </PropertyGroup>

  <PropertyGroup>
  </PropertyGroup>

  <Import Project="$(TopConfigDir)Build.Lib.props"/>
  <Import Project="$(DevDist)\$(sys-distid)\sys.refs.compression.props"/>
  <Import Project="$(DevDist)\$(common-distid)\json.refs.props"/>


  <ItemGroup  Label="refs/framework">
    <Reference Include="System.Drawing"/>
    <Reference Include="Microsoft.VisualBasic" />
  </ItemGroup>  

  <PropertyGroup>
    <PreBuildEvent>rc $(ProjectDir)IconStore\icons\icons.rc</PreBuildEvent>
    <Win32Resource>$(ProjectDir)IconStore\icons\icons.res</Win32Resource>
  </PropertyGroup>


</Project>