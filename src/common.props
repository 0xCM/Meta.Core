<Project>
  
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Authors>0xCM@gmail.com</Authors>
    <Version>1.1.0</Version>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <DevArea>metacore</DevArea>
    <area-distid>$(DevArea)</area-distid>
    <TopDir>$(DevAreaRoot)\$(DevArea)\</TopDir>
    <BuildRootDir>$(TopDir)bin\</BuildRootDir>
    <LibBuildDir>$(BuildRootDir)lib\</LibBuildDir>
    <OutputDir>$(LibBuildDir)</OutputDir>
    <OutputPath>$(LibBuildDir)</OutputPath>
    <IntermediateBuildDir>$(BuildRootDir)obj\$(MSBuildProjectName)\</IntermediateBuildDir>
    <BaseIntermediateOutputPath>$(IntermediateBuildDir)</BaseIntermediateOutputPath>
    <IntermediateOutputPath>$(IntermediateBuildDir)</IntermediateOutputPath>
    <NoWarn>1701;1702;1591</NoWarn>
    <CodeAnalysisRuleSet>$(MSBuildThisFileDirectory)common.ruleset</CodeAnalysisRuleSet>

  </PropertyGroup>

</Project>